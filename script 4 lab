      last_name ASC; 
      
      
      
      
      
      
      
      -- Завдання 5: 
SELECT 
* 
FROM 
    product 
WHERE
    category = 'NOTEBOOK' 
AND
    product_description 
LIKE 
   '%512GB%'
ORDER BY 
    product_name ASC; 







-- Завдання 6:
SELECT 
* 
FROM 
     product 
WHERE 
     category = 'NOTEBOOK'
OR 
     category LIKE 'D%'
AND 
     product_description 
LIKE 
     '%512GB%'
OR
    '%1TB%'
ORDER BY 
    category ASC; 



-- Завдання 7: 
SELECT 
* 
FROM 
    invoice 
WHERE 
    customer_id 
IS NULL; 
