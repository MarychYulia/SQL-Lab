  -- Завдання 4: 
      first_name, 
      last_name, 
      gender, 
      birth_date,
      phone_number 
FROM 
	customer 
WHERE 
	 gender = 'F'
AND 
	birth_date 
BETWEEN 
      '1990-01-01'
AND 
